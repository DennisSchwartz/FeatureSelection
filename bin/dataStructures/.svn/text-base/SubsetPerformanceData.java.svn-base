package dataStructures;

import java.util.BitSet;

/**
 * stores the performance of a specific set of features
 * @author Johannes Schoellhorn
 */
public class SubsetPerformanceData {
	private BitSet featureBitSet;
	private double performance;

	/**
	 * constructs a SubsetPerformanceData Object which can't be altered
	 * @param featureBitSet BitSet encoding the features of the subset
	 * @param performance performance of the subset
	 */
	public SubsetPerformanceData(BitSet featureBitSet, double performance) {
		this.featureBitSet = featureBitSet;
		this.performance = performance;
	}

	/**
	 * @return the featureSubset
	 */
	public BitSet getFeatureBitSet() {
		return featureBitSet;
	}

	/**
	 * @return the performance
	 */
	public double getPerformance() {
		return performance;
	}
	
}